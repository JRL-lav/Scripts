Login.MouseButton1Click:Connect(function()
    
    local data = game:GetService("HttpService"):JSONDecode(keylink)
    
	if KeyHolder.Text == data.Key then
        script(loadstring(game:HttpGet("https://raw.githubusercontent.com/Forgxtten/Phantom-Hub/main/Main-Loader/Hub.lua"))())
	    writefile("PhantomHub.json", keylink)  
		KeySystemUI:Destroy()  
		
	else
		Login.Text = "{Invalid}"
		wait(0.5)
		Login.Text = "Login"
	end
end)

GetKey.MouseButton1Click:Connect(function()
    setclipboard("http://sensei-joshy.xyz/")
	GetKey.Text = "Copied"
	wait(0.5)
	GetKey.Text = "Get Key"
end)


end


if isfile("PhantomHub.json") then
    
    local ff = readfile("PhantomHub.json")
    if ff == " " then
       keyshit() -- open key shit
    else
        local data = game:GetService("HttpService"):JSONDecode(readfile("PhantomHub.json"))
        local data2 = game:GetService("HttpService"):JSONDecode(keylink)

        if data.Key == data2.Key then
        script(loadstring(game:HttpGet("https://raw.githubusercontent.com/Forgxtten/Phantom-Hub/main/Main-Loader/Hub.lua"))())
        else
        keyshit()--open key ship
        end
    end
else
   keyshit() --open key shit
end


